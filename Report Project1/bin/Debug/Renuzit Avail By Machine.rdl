          <DataField>ent_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ent_id">
          <DataField>ent_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="parent_ent_id">
