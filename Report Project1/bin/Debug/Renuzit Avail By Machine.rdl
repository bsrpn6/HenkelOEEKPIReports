        </Field>
        <Field Name="shift_start_local">
          <DataField>shift_start_local</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Avail">
          <DataField>Avail</DataField>
          <rd:TypeName>System.Single</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
