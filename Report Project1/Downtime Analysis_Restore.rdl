    <DataSet Name="SampleData">
      <Fields>
        <Field Name="Scope">
          <DataField>Scope</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TextId">
          <DataField>TextId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="LanguageCode">
          <DataField>LanguageCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LocalizedText">
          <DataField>LocalizedText</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>aaReports</DataSourceName>
        <CommandText>IF EXISTS (SELECT LanguageCode 
              FROM aarsLayoutLocalizedText 
              WHERE LanguageCode= @UserLanguage
                AND Scope = 'ReportTemplate')

    SELECT TOP 10 *
       FROM aarsLayoutLocalizedText 
       WHERE LanguageCode=  @UserLanguage
         AND Scope = 'ReportTemplate'
    ORDER BY TextId

ELSE

    IF EXISTS (SELECT LanguageCode 
                  FROM aarsLayoutLocalizedText 
                  WHERE SUBSTRING(LanguageCode,1,charindex('-',LanguageCode))= SUBSTRING( @UserLanguage,1,charindex('-', @UserLanguage)) 
                    AND Scope = 'ReportTemplate')

        SELECT TOP 10 *
           FROM aarsLayoutLocalizedText 
           WHERE LanguageCode= (SELECT TOP 1 LanguageCode 
                                   FROM aarsLayoutLocalizedText 
                                   WHERE SUBSTRING(LanguageCode,1,charindex('-',LanguageCode))= SUBSTRING( @UserLanguage,1,charindex('-', @UserLanguage)) 
                                   AND Scope = 'ReportTemplate')
        AND Scope = 'ReportTemplate'
        ORDER BY TextId

    ELSE

        SELECT TOP 10 *
           FROM aarsLayoutLocalizedText 
           WHERE LanguageCode= 'en-us'
           AND Scope = 'ReportTemplate'
        Order By TextId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserLanguage">
            <Value>=Parameters!UserLanguage.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
