  <Code>Function CultureLanguageCode As String
   Dim oCulture As System.Globalization.CultureInfo
   CultureLanguageCode =   LCase(oCulture.CurrentUICulture.Name)
End Function

Function ClipString(ByVal aString As String, ByVal Max As Integer) As String
    aString = Trim(aString)
    If Len(aString) &gt; Max And Max &gt; 0 Then
        ClipString = Left(aString, Max - 1) &amp; "…"
    Else
        ClipString = aString
    End If
End Function

Function SectionHidden(ByVal SectionList As Parameter, ByVal SectionName As String) As Boolean

    Dim Position As Integer = 0
    Dim IsVisible As Boolean = False

    Do
        If SectionList.Value(Position) = "ALL" Or SectionList.Value(Position) = SectionName Then
            IsVisible = True
        End If
        Position = Position + 1
    Loop While Position &lt; SectionList.Value.Length And Not (IsVisible)

    SectionHidden = Not (IsVisible)
End Function

Function ChartColor(ByVal AllParameters As Parameters, ByVal Index As Integer) As String
     Dim BaseColor As Integer = 18
     Dim BaseGray As Integer = 30
     Dim IsGray As Boolean = AllParameters("LayoutIsGrayScale").Value

     If IsGray Then
        ChartColor = AllParameters("LayoutColorCodes").Value( BaseGray + Index )
    Else
        ChartColor = AllParameters("LayoutColorCodes").Value( BaseColor + Index )
    End If
End Function

Function TrendURL( ByVal SystemParameters As Parameter, ReportServerUrl As String, TagName As String, StartTime As DateTime, EndTime As DateTime ) As String
    Dim ServerStart As Integer
    If (ReportServerUrl = "")
       TrendURL = SystemParameters.Value(4) ' Hostname
    Else
       ServerStart = InStr( ReportServerUrl, "//" ) + 2
       TrendURL = Mid(ReportServerUrl, 1, InStr( ServerStart, ReportServerUrl, "/" )-1 )
    End If
    TrendURL = TrendURL &amp;  SystemParameters.Value(3) ' Virtual directory
    TrendURL = TrendURL &amp; "/Applications/HistClientReportingSite/redirect.asp?page=Trend.aspx"
    TrendURL = TrendURL &amp; "?SV=1"
    TrendURL = TrendURL &amp;  SystemParameters.Value(5)
    TrendURL = TrendURL &amp; "&amp;Tags=" &amp; TagName
    TrendURL = TrendURL &amp; "&amp;Start=" &amp; Format( StartTime, "yyyy-MM-dd hh:mm:ss" )
    TrendURL = TrendURL &amp; "&amp;End=" &amp; Format( EndTime, "yyyy-MM-dd hh:mm:ss" )
End Function

Function FormatValue(ByVal PV As Object, ByVal Precision As Integer) As String
    If IsNumeric(PV) Then
        Dim Places As Integer = 5 - CInt(System.Math.Log10(System.Math.Abs(CDbl(PV)))) + Precision
        If Places &lt; 0 Then
	Places = 1
        End If
        FormatValue = FormatNumber(CDbl(PV), Places )
    Else
        FormatValue = ClipString(PV,14)
    End If
End Function

Public Function SubstituteSingleQuote(ByVal strText As String) As String
        Dim result As String
        result = ""

        If (IsNothing(strText) Or Len(strText) = 0) Then
            SubstituteSingleQuote = ""
        Else
            Dim lCount As Integer
            lCount = Len(strText)
            Dim i As Integer
            Dim thisChar As String
            For i = 1 To lCount
                thisChar = Mid(strText, i, 1)
                If (thisChar = "'") Then
                    result = result + "'"
                End If
                result = result + thisChar
            Next
            SubstituteSingleQuote = result
        End If
    End Function

Public Function SplitStr(ByVal ds As Object()) As String
    Dim i As Integer
    Dim res As String
    res = ""
    i = 0
    For i = 0 To (ds.Length - 1)
        If i &gt; 0 Then
            res = res &amp; ", "
        End If
        If (ds(i) &lt;&gt; "&lt;Blank&gt;") Then
            res = res &amp; "N'" &amp; SubstituteSingleQuote(CStr(ds(i))) &amp;"'"
        Else
            res = res &amp; "N''"
        End If
    Next i
    If (res.Length = 0) Then
        res = "null"
    End If
    SplitStr = res
End Function

Public Function GetWhereClause(ByVal strCond As String) As String
    Dim len As Integer
    Dim res As String
    res = ""
    If (strCond &lt;&gt; Nothing) Then
        res = res &amp; "WHERE "
        If (strCond.EndsWith("AND ")) Then
            len = strCond.Length()
            res = res &amp; strCond.Remove(len - 4, 4)
        Else
            res = res &amp; strCond
        End If
    End If
    GetWhereClause = res
End Function

Public Function GetExtendedWhereClause(ByVal strCond1 As String, ByVal strCond2 As String) As String
    Dim len As Integer
    Dim res As String
    res = GetWhereClause(strCond1)
    If (res &lt;&gt; Nothing) Then
        If (strCond2 &lt;&gt; Nothing) Then
            res = res &amp; "AND  " &amp; strCond2
        End If
    Else
        res = "Where " &amp; strCond2
    End If
    GetExtendedWhereClause = res
End Function

Public Function Test(ByVal y As Object()) As String
    Dim x As String
    x = SplitStr(y)
    Test = x
End Function

Public Function SerNumbWhereCondition(ByVal strQueryLimit As String, _
                                           ByVal bSelectSpecific As Boolean, _
                                           ByVal strPattern As String, _
                                           ByVal nValidCnt As Integer, _
                                           ByVal nSelectedCnt As Integer, _
                                           ByVal PASelectedValues As Object()) As String

    strPattern = ConvertPattern(strPattern)
    strQueryLimit = ConvertQueryLimit(strQueryLimit)

    Dim res As String
    res = ""

    If (PASelectedValues(0) &lt;&gt; "ALL") Then
        res = String.Format("({0})", SplitStr(PASelectedValues))
    Else
        If (strQueryLimit = "" Or bSelectSpecific = False) Then
            If (strPattern &lt;&gt; "") Then
                res = res &amp; String.Format("(Select distinct SerialNumber from SerialNumber where SerialNumber LIKE '{0}') ", strPattern)
            End If
        Else
            res = String.Format("(Select Top {0} SerialNumber from SerialNumber ", _
                                strQueryLimit)
            If (strPattern &lt;&gt; "") Then
                res = res &amp; String.Format("where SerialNumber like '{0}'", _
                                          strPattern)
            End If
            res = res &amp; "order by SerialNumber) "
        End If
    End If
    SerNumbWhereCondition = res
End Function


Public Function CreateCountDataset(ByVal strQueryLimit As String, _
                                           ByVal bSelectSpecific As Boolean, _
                                           ByVal strPattern As String, _
                                           ByVal strField As String, _
                                           ByVal strView As String) As String

    strPattern = ConvertPattern(strPattern)
    strQueryLimit = ConvertQueryLimit(strQueryLimit)

    Dim res As String
    res = ""
    If (Not bSelectSpecific) Then
        res = "SELECT 1 as PACount"
    Else
        If (strQueryLimit &lt;&gt; "" And bSelectSpecific) Then
            res = String.Format("SELECT (CASE WHEN(COUNT({0})&lt;{1}) THEN COUNT({2}) ELSE {3} END) as PACount FROM {4} ", _
                                strField, _
                                strQueryLimit, _
                                strField, _
                                strQueryLimit, _
                                strView)
            If (strPattern &lt;&gt; "") Then
                res = res &amp; String.Format("WHERE {0} LIKE '{1}' AND {2} IS NOT NULL", _
                                          strField, _
                                          strPattern, _
                                          strField)
            Else
                res = res &amp; String.Format(" WHERE {0} IS NOT NULL", strField)
            End If
        Else
            res = String.Format("SELECT COUNT({0}) AS PACount FROM {1} ", strField, strView)
            If (strPattern &lt;&gt; "") Then
                res = res &amp; String.Format("WHERE {0} LIKE '{1}' AND {2} IS NOT NULL", _
                                          strField, _
                                          strPattern, _
                                          strField)
            Else
                res = res &amp; String.Format(" WHERE {0} IS NOT NULL", strField)
            End If
        End If
    End If
    CreateCountDataset = res
End Function

Public Function CreateValidValuesDataset(ByVal bQueryScope As Boolean, _
                                           ByVal bSelectSpecific As Boolean, _
                                           ByVal strPattern As String, _
                                           ByVal strField As String, _
                                           ByVal strView As String) As String

    strPattern = ConvertPattern(strPattern)
    Dim strQueryLimit As String
    strQueryLimit = ""
    
    Dim res As String
    res = ""
    If (bQueryScope = False) Then
    If (bSelectSpecific = False And strPattern = "") Then
        res = String.Format("SELECT N'ALL' as {0}, 0 as Id ", strField)
    Else
        res = "SELECT "
        If (strQueryLimit &lt;&gt; "") Then
            res = res &amp; String.Format("Top {0} ", strQueryLimit)
        End If

        res = res &amp; String.Format(" (CASE WHEN(Len({0})&gt;0) THEN {1} ELSE N'&lt;Blank&gt;' END) AS {2}, 1 as Id From {3} ", _
                                 strField, _
                                 strField, _
                                 strField, _
                                 strView)
        
            If (strPattern &lt;&gt; "") Then
                res = res &amp; String.Format("WHERE {0} {1} AND {2} IS NOT NULL", _
                                      strField, _
                                      strPattern, _
                                      strField)
            Else
                res = res &amp; String.Format(" WHERE {0} IS NOT NULL", strField)
            End If
            res = res &amp; String.Format(" UNION Select N'ALL' AS {0}, 0 as Id order by Id, {1}", strField, strField)
        End If
    Else
        res = String.Format("SELECT N'ALL' as {0}, 0 as Id ", strField)
    End If
    
    CreateValidValuesDataset = res
End Function

 Public Function GetFilterString(ByVal nSelectedCnt As Integer, _
                                   ByVal PAPattern As String, _
                                   ByVal PASelectedValues As Object()) As String

        'PAPattern = ConvertPattern(PAPattern)

        Dim res As String
        Dim temp As String
        res = ""
        
        Dim i As Integer
        Dim nUpperBound As Integer

        nUpperBound = 10
        If (nSelectedCnt &lt; 10) Then
            nUpperBound = nSelectedCnt
        End If

        If (PASelectedValues(0).ToString() &lt;&gt; "ALL") Then
            For i = 1 To nUpperBound
                If (PASelectedValues(i - 1).ToString() = "") Then
                    temp = "&lt;Blank&gt;"
                Else
                    temp = PASelectedValues(i - 1).ToString()
                End If
                If (i = 1) Then
                    res = temp
                Else
                    res = res &amp; ", " &amp; temp
                End If
            Next
            If (nSelectedCnt &gt; 10) Then
                res = res &amp; "..."
            End If
        Else
            If (PAPattern &lt;&gt; "") Then
                res = "Pattern: " &amp; PAPattern
            End If
        End If
        GetFilterString = res
    End Function
Public Function ConvertQueryLimit(ByVal strQueryLimit As String) As String
    Dim Limit As Integer
    Limit = 0
    If Not (IsNothing(strQueryLimit) Or Len(strQueryLimit) = 0) Then
        Try
            Limit = Convert.ToInt64(strQueryLimit)
            If Limit &lt; 0 Then
                Limit = 0
            End If
        Catch ex As Exception
            Limit = 0
        End Try
    End If
    If (Limit &gt; 0) Then
        ConvertQueryLimit = Limit.ToString()
    Else
        ConvertQueryLimit = ""
    End If
End Function

Public Function ConvertPattern(ByVal strPattern As String) As String
    Dim bEscape As Boolean
    Dim result As String
    result = ""

    bEscape = False
    If (IsNothing(strPattern) Or Len(strPattern) = 0) Then
        ConvertPattern = ""
    Else
        Dim escChars As String
        escChars = "*%?_\"
        Dim lCount As Integer
        lCount = Len(strPattern)
        Dim i As Integer
        Dim wildCharIndex As Integer
        Dim thisChar As String
        For i = 1 To lCount
            thisChar = Mid(strPattern, i, 1)
            If (thisChar &lt;&gt; "'") Then
                wildCharIndex = escChars.IndexOf(thisChar)
                If (wildCharIndex &gt;= 0) Then
                    If (i &gt; 1) Then
                        If (Mid(strPattern, i - 1, 1) = "\") Then
                            bEscape = True
                        End If
                    End If
                    If (thisChar = "?") Then
                        thisChar = "_"
                    ElseIf (thisChar = "*") Then
                        thisChar = "%"
                    End If
                End If
                result = result + thisChar
            Else
                result = result + "''"
            End If
        Next i
        If (bEscape) Then
            result = "LIKE '" + result + "' ESCAPE '\' "
        Else
            result = "LIKE '" + result + "'"
        End If
        ConvertPattern = result
    End If
End Function


Public Function WhereCondition(ByVal strPattern As String, _
                                ByVal strField As String, _
                                ByVal PASelectedValues As Object()) As String

    strPattern = ConvertPattern(strPattern)

    Dim res As String
    res = ""

    If (PASelectedValues(0) &lt;&gt; "ALL") Then
        res = String.Format("(DT.{0} in ({1})) AND ", strField, SplitStr(PASelectedValues))
    Else
        If (strPattern &lt;&gt; "") Then
            res = String.Format("(DT.{0} {1} ) AND ", strField, strPattern)
        End If
    End If
    WhereCondition = res
End Function
