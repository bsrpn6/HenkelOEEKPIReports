    <DataSet Name="SampleMatrixData">
      <Fields>
        <Field Name="A">
          <DataField>A</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="B">
          <DataField>B</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="C">
          <DataField>C</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>aaReports</DataSourceName>
        <CommandText>SELECT A.ColorID AS A,B.ColorID AS B,A.ColorID+B.ColorID AS C
FROM (SELECT TOP 5 ColorID FROM aarsLayoutColorCodes) AS A CROSS JOIN (SELECT TOP 5 ColorID*10 AS ColorID FROM aarsLayoutColorCodes) AS B</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="SampleChartData">
      <Fields>
